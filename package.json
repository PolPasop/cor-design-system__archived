{
  "name": "cor-design-system",
  "version": "0.1.0",
  "main": "gulpfile.js",
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@frctl/fractal": "^1.5.4",
    "@frctl/nunjucks": "^2.0.8",
    "concurrently": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-watch": "^5.0.1",
    "sass": "^1.32.8",
    "through2": "^4.0.2"
  },
  "scripts": {
    "sass:tokens": "npx theo src/site/_data/tokens.json --format scss,cssmodules.css --dest assets/scss/",
    "sass:process": "sass assets/scss/global.scss assets/css/global.css --style=compressed",
    "build": "gulp build && echo \"üèã Build completed\" && exit 0",
    "dev": "gulp dev",
    "cms:precompile": "",
    "start": "concurrently \"npm run sass:process -- --watch\" \"chokidar \\\"./**/*.scss\\\" -c \\\"npm run cms:precompile\\\"\" \"npm run serve\"",
    "serve": "cross-env ELEVENTY_ENV=development npx eleventy --serve",
    "fractal:start": "fractal start --sync",
    "fractal:build": "fractal build"
  },
  "devDependencies": {
    "@11ty/eleventy-navigation": "^0.1.6",
    "chokidar-cli": "^2.1.0",
    "cross-env": "^7.0.3"
  }
}
